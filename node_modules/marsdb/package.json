    "gulp-if": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.5.1",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.3.4",
    "mocha-lcov-reporter": "^1.0.0",
    "mocha-phantomjs": "^4.0.2",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0",
