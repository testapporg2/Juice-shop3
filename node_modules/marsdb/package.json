  },
  "contributors": [
    "Artem Artemev"
  ],
  "description": "MarsDB is a lightweight client-side MongoDB-like database, Promise based, written in ES6",
  "keywords": [
    "database",
    "datastore",
    "embedded",
    "levelup",
    "mongoose",
    "linvodb3",
    "nedb"
  ],
  "homepage": "https://github.com/c58/marsdb",
  "repository": {
    "type": "git",
    "url": "git@github.com:c58/marsdb.git"
  },
  "dependencies": {
    "check-types": "^6.0.0",
    "double-ended-queue": "^0.9.7",
    "eventemitter3": "1.1.1",
    "fast.js": "^0.1.1",
    "geojson-utils": "^1.1.0",
    "invariant": "^2.2.0"
  },
  "devDependencies": {
    "core-js": "^2.0.1",
    "lodash": "3.10.x",
    "babel-cli": "^6.3.17",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babelify": "^7.2.0",
    "brfs": "^1.4.1",
    "browserify": "^13.0.0",
    "bulk-require": "^0.2.1",
    "bulkify": "^1.1.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "coveralls": "^2.11.6",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "fbjs-scripts": "^0.5.0",
