{
  "name": "fuzzball",
  "version": "1.4.0",
  "description": "Fuzzy string matching algorithms and utilities, port of the fuzzywuzzy Python library.",
  "main": "fuzzball.js",
  "types": "fuzzball.d.ts",
  "files": [
    "fuzzball.js",
    "fuzzball.d.ts",
    "dist",
    "lite",
    "ultra_lite",
    "lib"
  ],
  "dependencies": {
    "heap": ">=0.2.0",
    "setimmediate": "^1.0.5",
    "string.fromcodepoint": "^0.2.1",
    "string.prototype.codepointat": "^0.2.0"
  },
  "devDependencies": {
    "browserify": "^15.2.0",
    "cached-path-relative": "^1.0.2",
    "jsdoc": "^3.6.3",
    "jsdoc-to-markdown": "^5.0.3",
    "lodash-cli": "^4.17.5",
    "mocha": "^7.1.1",
    "uglify-js": "^3.12.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nol13/fuzzball.js.git"
  },
  "keywords": [
    "levenshtein",
    "string",
    "fuzzy",
    "matching",
    "search",
    "ratio",
    "distance",
    "compare",
    "wildcard",
    "dedupe"
  ],
  "author": "Nolan Kaplan <nolan@nolankaplan.com>",
  "license": "GPL-2.0",
  "scripts": {
    "jsdoc2md": "./node_modules/.bin/jsdoc2md --template ./jsdocs/README.hbs fuzzball.js > jsdocs/fuzzball.md && ./node_modules/.bin/jsdoc2md lib/process.js > jsdocs/process.md",
    "lodash:reg": "./node_modules/lodash-cli/bin/lodash include=intersection,difference,uniq,intersectionWith,differenceWith,uniqWith,toArray,partialRight,keys,isArray,forEach,orderBy -p -o ./lib/lodash.custom.min.js",
    "lodash:lite": "./node_modules/lodash-cli/bin/lodash include=intersection,difference,uniq,toArray -p -o ./lite/lodash.custom.min.js",
    "lodash:ultralite": "./node_modules/lodash-cli/bin/lodash include=intersection,difference,uniq -p -o ./ultra_lite/lodash.custom.min.js",
    "build:reg:old": "./node_modules/.bin/browserify -r ./fuzzball.js:fuzzball | ./node_modules/uglify-js/bin/uglifyjs > dist/fuzzball_browser.min.js",
    "build:lite:old": "./node_modules/.bin/browserify -r ./lite/fuzzball_lite.js:fuzzball | ./node_modules/uglify-js/bin/uglifyjs > lite/fuzzball_lite_browser.min.js",
    "build:reg": "./node_modules/.bin/browserify ./fuzzball.js --standalone fuzzball | ./node_modules/uglify-js/bin/uglifyjs > dist/fuzzball.umd.min.js",
    "build:lite": "./node_modules/.bin/browserify ./lite/fuzzball_lite.js --standalone fuzzball | ./node_modules/uglify-js/bin/uglifyjs > lite/fuzzball_lite.umd.min.js",
    "build:ultralite": "./node_modules/.bin/browserify ./ultra_lite/fuzzball_ultra_lite.js --standalone fuzzball | ./node_modules/uglify-js/bin/uglifyjs > ultra_lite/fuzzball_ultra_lite.umd.min.js",
    "build:demo": "rm -rf docs && cd demo-app && npm run build && cp -a build/. ../docs/",
    "build": "npm run lodash:ultralite && npm run lodash:lite && npm run lodash:reg && npm run build:reg && npm run build:lite && npm run build:ultralite && npm run build:reg:old && npm run build:lite:old && npm run jsdoc2md && npm run test:build && npm run build:demo",
    "test": "mocha --use_strict",
    "test:build": "testenv=build npm run test"
  },
  "runkitExample": "var fuzz = require('fuzzball');"
}
